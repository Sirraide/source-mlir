/// RUN: %srcc %s --hlir | FileCheck %s
proc i extern -> int;
proc consume (int) extern;

int a = i();
int b = i();

consume a + b;   /// CHECK: hlir.add
consume a - b;   /// CHECK: hlir.sub
consume a * b;   /// CHECK: hlir.mul
consume a ** b;  /// CHECK: hlir.exp
consume a / b;   /// CHECK: hlir.div
consume a % b;   /// CHECK: hlir.rem
consume a and b; /// CHECK: hlir.and
consume a or b;  /// CHECK: hlir.or
consume a xor b; /// CHECK: hlir.xor
consume a << b;  /// CHECK: hlir.shl
consume a >> b;  /// CHECK: hlir.shr
consume a >>> b; /// CHECK: hlir.sar
consume a == b;  /// CHECK: hlir.eq
consume a != b;  /// CHECK: hlir.ne
consume a < b;   /// CHECK: hlir.lt
consume a <= b;  /// CHECK: hlir.le
consume a > b;   /// CHECK: hlir.gt
consume a >= b;  /// CHECK: hlir.ge




