/// RUN: %srcc %s --hlir | FileCheck %s
proc i extern -> int;
proc consume (int) extern;

int a = i();
int b = i();

consume a + b;   /// CHECK: arith.addi
consume a - b;   /// CHECK: arith.subi
consume a * b;   /// CHECK: arith.muli
consume a ** b;  /// CHECK: hlir.expi
consume a / b;   /// CHECK: arith.divsi
consume a % b;   /// CHECK: arith.remsi
consume a and b; /// CHECK: arith.andi
consume a or b;  /// CHECK: arith.ori
consume a xor b; /// CHECK: arith.xori
consume a << b;  /// CHECK: arith.shli
consume a >> b;  /// CHECK: arith.shrsi
consume a >>> b; /// CHECK: arith.shrui
consume a == b;  /// CHECK: arith.cmpi eq
consume a != b;  /// CHECK: arith.cmpi ne
consume a < b;   /// CHECK: arith.cmpi slt
consume a <= b;  /// CHECK: arith.cmpi sle
consume a > b;   /// CHECK: arith.cmpi sgt
consume a >= b;  /// CHECK: arith.cmpi sge




